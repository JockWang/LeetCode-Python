   "source": [
    "class Solution:\n",
    "    def defangIPaddr(self, address: str) -> str:\n",
    "        if len(address) != 0:\n",
    "            return address.replace('.','[.]')\n",
    "        return address\n",
    "s = Solution()\n",
    "s.defangIPaddr('255.100.50.0')"
