   "source": [
    "class Solution:\n",
    "    def canJump(self, nums: [int]) -> bool:\n",
    "        position = len(nums)-1\n",
    "        for i in range(len(nums)-1,-1,-1):\n",
    "            if i+nums[i] >= position:\n",
    "                position = i\n",
    "        return position==0"
   ]
