    "class Solution:\n",
    "    def mySqrt(self, x: int) -> int:\n",
    "        if x < 2:\n",
    "            return x\n",
    "        x0 = x\n",
    "        x1 = (x0+x/x0)/2\n",
    "        while abs(x0-x1) >= 1:\n",
    "            x0 = x1\n",
    "            x1 = (x0+x/x0)/2\n",
    "        return int(x1)\n",
    "s = Solution()\n",
    "s.mySqrt(8)"
   ]
