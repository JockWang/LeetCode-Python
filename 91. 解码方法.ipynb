    "class Solution:\n",
    "    def numDecodings(self, s: str) -> int:\n",
    "        pp, p = 1, int(s[0] != '0')\n",
    "        for i in range(1, len(s)):\n",
    "            pp, p = p, pp * (9 < int(s[i-1:i+1]) <= 26) + p * (int(s[i]) > 0)\n",
    "        return p\n",
    "s = Solution()\n",
    "s.numDecodings('12120')"
   ]
